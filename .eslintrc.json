{
  "env": {
    "browser": true,
    "jquery": true,
    "greasemonkey": true,
    "es6": true
  },
  "globals": {
    "saveAs": false,
    "JSZip": false,
    "jEpub": false
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": 0
  }
}
